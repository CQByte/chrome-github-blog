{
	"name": "BlogHub",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "在chrome中编写、管理 GitHub page",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "off.png",
		"default_title": "新建文章",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"on.png","off.png"
	]
}